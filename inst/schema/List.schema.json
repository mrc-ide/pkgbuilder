{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "id": {
                "type": "string"
            },
            "status": {
                "type": "string",
                "enum": ["READY", "WORKING", "COMPLETE", "ERROR", "UNKNOWN"]
            }
        },
        "required": ["id", "status"],
        "additionalProperties": false
    }
}
